                <TextBox Width="70" VerticalAlignment="Center"></TextBox>
                <Button Content="下发" Margin="5,0,0,0"></Button>
            </StackPanel>

            <StackPanel Orientation="Horizontal" Margin="4,4,0,0">
                <TextBlock Text="桨区1X坐标值：" VerticalAlignment="Center"></TextBlock>
